<!-- Start Post List Wrapper  -->
<div class="axil-post-list-area axil-section-gap mt--30">
    <div class="container">
        <div class="row">
            <% for(let celebrityCategory of categories.celebrityCategories) { %>
                <!-- Start Post List  -->
                <div class="col-lg-4 col-xl-4 mt--30">
                    <div class="content-block post-list-view">
                        <div class="post-content">
                            <div class="post-cat">
                                <div class="post-cat-list">
                                    <a class="hover-flip-item-wrapper" href="#">
                                        <span class="hover-flip-item">
                                            <span data-text="مشاهير">مشاهير</span>
                                        </span>
                                    </a>
                                </div>
                            </div>
                            <h4 class="title">
                                <a href="/ar/celebrities/c/<%= celebrityCategory.url_slug %>">
                                    <%= celebrityCategory.title %>
                                </a>
                            </h4>
                            <div class="post-meta-wrapper">
                                <div class="post-meta">
                                    <div class="content">
                                        <ul class="post-meta-list">
                                            <li>
                                                <%= celebrityCategory.created_date_time %>
                                            </li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- End Post List  -->
                <% } %>

